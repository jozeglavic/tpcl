	<text>
		<body><div type="letter"><div type="letter">
			<pb facs="#facs_1" xml:id="LJU296_35_I_P05_001.tif" n="1" />
			<opener rend="center"><salute><lb facs="#facs_1_r1l1" n="N001" /><hi rend="italic">Meine theuerste Josephine!</hi></salute></opener>
			<p facs="#facs_1_r1">				
				<lb facs="#facs_1_r1l2" n="N002" />Ich mache mir zum größten Vergnugen
				<lb facs="#facs_1_r1l3" n="N003" />beygehenden kleinen unschuldigen <hi rend="italic">Deliquenten
				<lb facs="#facs_1_r1l4" n="N004" />Ihrer</hi> milden Behandlung anzuvertrauen;
				<lb facs="#facs_1_r1l5" n="N005" />er ist mein Zögling von Jugend auf, und
				<lb facs="#facs_1_r1l6" n="N006" />lohn so ziemlich meine ihm geopferten
				<lb facs="#facs_1_r1l7" n="N007" /><hi rend="italic">Instructionen</hi>. 				
			</p>
			<closer>
				<lb facs="#facs_1_r1l8" n="N008" />Ich bitte dieß kleine Andenken güttigst 
				<lb facs="#facs_1_r1l9" n="N009" />anzunehmen, von Ihren
				<lb facs="#facs_1_r1l10" n="N010" />anstrichtigsten ergebensten 
				<lb facs="#facs_1_r1l11" n="N011" /><persName ref="persName.xml#fidelij">Fidelis Terpinz</persName>
			</closer>
			<pb facs="#facs_2" xml:id="LJU296_35_I_P05_002.tif" n="2" />
		</div></div></body>
	</text>
